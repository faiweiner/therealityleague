<div>
	<%= @league_players %>
	<span class="participating-leagues">
		<h2>My Rosters and Leagues</h2>
		<%= link_to(content_tag(:button, "Edit Rosters"), rosters_path) %>

		<h2>League I'm participating in</h2>
		<ul>
		<%= @current_user.leagues.length %>
		<% @leagues.each do |league| %>
			<li><%= league.name %></li>
		<% end %>
		</ul>
	</span>
	<table id="my_leagues_table">
		<thead>
			<tr>
				<th>Name</th>
				<th colspan="3">Show</th>
			</tr>
		</thead>

		<tbody>
			<% @leagues.each do |league| %>
				<tr>
					<td><%= league.name %></td>
					<td><%= link_to 'Show', league %></td>
					<td><%= link_to 'Edit', edit_league_path(league) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>


