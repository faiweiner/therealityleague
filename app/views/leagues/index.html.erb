<div>
	<%= @league_players %>
	<span class="participating-leagues">
		<h2>My Rosters and Leagues (<%= @current_user.leagues.length %>)</h2>
		<table id="my_leagues_table">
			<thead>
				<tr>
					<th>League Name</th>
					<th colspan="3">Show</th>
				</tr>
			</thead>
			<tbody>
				<% @leagues.each do |league| %>
					<tr>
						<td><%= league.name %></td>
						<td><%= league.show_id %></td>
						<td>
							<%= @comm_leagues.name %>
							<% if @comm_leagues.include? ("Hiya") %>
								FIXME INSERT SOMETHING HERE!!!
							<% end %>
						</td>
						<td><%= link_to(content_tag(:button, "Edit Roster"), rosters_path) %></td>
						
					</tr>
				<% end %>
			</tbody>
		</table>
	</span>
</div>


  <!-- $("input:text:visible:first").focus(); -->

<div name="commissioning-leagues">
	<h2>Commissioned Leagues</h2>
	<table id="comm_leagues_table"
		<thead>
			<tr>
				<th>League Name</th>
				<th colspan="3">Show</th>
			</tr>
		</thead>

		<tbody>
			<% @comm_leagues.each do |league| %>
				<tr>
					<td><%= league.name %></td>
					<td><%= league.show_id %></td>
					<td><%= link_to 'Show', league %></td>
					<td><%= link_to 'Edit', edit_league_path(league) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>

<%= render partial: "league_access_nav" %>
