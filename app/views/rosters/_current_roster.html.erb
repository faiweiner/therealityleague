<% @selected_contestants.each do |contestant| %>
	<div class="col-md-3">
		<div class="thumbnail thumbnail-current-roster">
			<i class="remove-button fa fa-times overlap-button" data-contestant-id="<%= contestant.id %>" data-roster-id="<%= @roster.id %>"></i>
			<%= image_tag contestant.image %>
			<div class="caption">
				<%= contestant.name %>
			</div>
		</div>
	</div>
<% end %>