<div class="panel panel-default">
	<div class="panel-heading">
		<div class="panel-title">
			<h3><%= @show.name %></h3>
			<%= link_to "Back to Show", show_path(@show.id), class:"btn btn-default" %>
			<%= link_to "edit path", root_path, class:"btn btn-default" %>
		</div>
	</div>
	<div class="panel-body">
		<div class="row">
			<div class="panel-body">

				<a href="/pages/about" id="username" data-type="text" data-pk="1" data-url="/post" data-title="Enter username">superuser</a>

				<table class="table table-hover">
					<thead>
						<tr>
							<th>Image</th>
							<th>Name</th>
							<th>Age</th>
							<th>Occupation</th>
							<th>Description</th>
						</tr>
					</thead>
					<tbody>
						<% @contestants.each do |contestant| %>
							<tr>
								<td><%= image_tag contestant.image, class:"img-thumbnail thumbnail-sm" %></td>
								<td class="edit name-cell" data-pk="<%= contestant.id %>" data-type="text" data-contestant-id="<%= contestant.id %>" data-url="/post" data-title="Enter name"><%= contestant.name %></td>




								
								<td class="edit age-cell" data-pk="<%= contestant.id %>" data-type="integer" data-contestant-id="<%= contestant.id %>" data-url="/post" data-title="Enter age"><%= contestant.age %></td>
								<td class="edit occupation-cell" data-pk="<%= contestant.id %>" data-type="text" data-contestant-id="<%= contestant.id %>" data-url="/post" data-title="Enter occupation"><%= contestant.occupation %></td>
								<td class="edit description-cell" data-pk="<%= contestant.id %>" data-type="text" data-contestant-id="<%= contestant.id %>" data-url="/post" data-title="Enter description"><%= contestant.description %></td>
							</tr>
						<% end %>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>

