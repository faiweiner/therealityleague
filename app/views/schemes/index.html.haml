#manage-schemes.panel.panel-default
	.panel-body
		.col-lg-12
			%h2 Manage Point Schemes
	.panel-body
		.col-lg-12#shows-panel
			= render "shows_menu"
		.col-lg-3#scheme-form
			= render partial: "form"
		.col-lg-9#current-scheme-board
			= render partial: "display_schemes", locals: { schemes: @schemes }